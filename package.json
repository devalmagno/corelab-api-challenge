{
  "name": "corelab-api-challenge",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "node ace migration:run && node ace serve --watch",
    "build": "node ace build --production",
    "start": "node build/server.js",
    "migration": "node ace migration:run",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write .",
    "test": "node ace test",
    "test:watch": "npm test --watch"
  },
  "eslintConfig": {
    "extends": [
      "plugin:adonis/typescriptApp",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error"
      ]
    }
  },
  "eslintIgnore": [
    "build"
  ],
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "useTabs": false,
    "quoteProps": "consistent",
    "bracketSpacing": true,
    "arrowParens": "always",
    "printWidth": 100
  },
  "devDependencies": {
    "@adonisjs/assembler": "^5.6.2",
    "@japa/preset-adonis": "^1.0.15",
    "@japa/runner": "^2.0.7",
    "@types/uuid": "^9.0.4",
    "adonis-preset-ts": "^2.1.0",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-adonis": "^2.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "pino-pretty": "^7.6.1",
    "prettier": "^2.6.2",
    "typescript": "~4.6",
    "youch": "^3.2.0",
    "youch-terminal": "^2.1.3"
  },
  "dependencies": {
    "@adonisjs/core": "^5.7.6",
    "@adonisjs/lucid": "^18.4.2",
    "@adonisjs/repl": "^3.1.10",
    "luxon": "^3.4.3",
    "pg": "^8.11.3",
    "proxy-addr": "^2.0.7",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.21",
    "uuid": "^9.0.1"
  },
  "engines": {
    "node": "^16.15.0",
    "npm": "^8.5.5"
  }
}
